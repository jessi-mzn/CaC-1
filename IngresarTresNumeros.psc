Algoritmo IngresarTresNumeros
	//zona de definiciones
	Definir num1, num2, num3 Como Real;
	
	
	//zona de algoritmo
	Escribir "Ingrese tres numeros";
	Leer num1, num2, num3;
	Escribir "*****************";
	Escribir "Los numeros ingresados son:";
	Escribir num1;
	Escribir num1;
	Escribir num1;
	Escribir num1, ", ", num2, "," ,num3;
	Escribir "*****************";
	
	
FinAlgoritmo
